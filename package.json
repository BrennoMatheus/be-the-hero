  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/jest": "^26.0.3",
    "@types/node": "^14.0.14",
    "@types/supertest": "^2.0.9",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "eslint": "^7.3.1",
    "eslint-config-standard-with-typescript": "^18.0.2",
    "eslint-plugin-import": "^2.21.2",
